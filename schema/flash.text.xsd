<xs:complexContent>
<xs:extension base="empty:RootObjectTypeSchemaType">
<xs:all minOccurs='0'>
<xs:element minOccurs="0" name="align" type="xs:string"/>
<xs:element minOccurs="0" name="blockIndent" type="xs:string"/>
<xs:element minOccurs="0" name="bold" type="xs:string"/>
