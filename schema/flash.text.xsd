<xs:attribute name="descent" type="xs:double"/>
<xs:attribute name="height" type="xs:double"/>
<xs:attribute name="leading" type="xs:double"/>
<xs:attribute name="width" type="xs:double"/>
<xs:attribute name="x" type="xs:double"/>
</xs:extension>
</xs:complexContent>
</xs:complexType>

